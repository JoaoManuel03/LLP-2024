#include <stdio.h>
int main(void) {
  int x = 11;
  if (0 <= x && x <= 10) {
    printf("insuficiente");
  }
  if (10 < x && x <= 40) {
    printf("regular");
  }
  if (40 < x && x <= 70) {
    printf("bom");
  }
  if (70 < x && x <= 90) {
    printf("otimo");
  }
  if (90 < x && x <= 100) {
    printf("excelente");
  }
  else if ( 100 < x){
    printf("pontuação maxima alcançada");
  }
else if (100 < x){
  printf("pontuação negativada");
}
 
  return 0;
}